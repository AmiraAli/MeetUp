<div class="container">
<div class="row">
<ul class="nav nav-tabs">
  <li role="presentation"><a href="http://localhost:3000/groups">Groups</a></li>
  <li role="presentation"><a href="http://localhost:3000/events">Events</a></li>
  <li role="presentation"><a href="http://localhost:3000/interests">Interests</a></li>
  <li role="presentation" class="active"><a href="http://localhost:3000/users">Users</a></li>
</ul>
</div>
<div class="row">
    <p id="notice"><%= notice %></p>
    <div class="col-md-offset-10 ">
      <%= link_to 'New User', new_user_path %>
    </div>
    <div class="col-md-12">
      <h1>
        <span class="label label-default">Listing Users</span>
      </h1>
      <div class="table-responsive">
        <table class="table table-hover">
  <thead>
    <tr>
      <th>Username</th>
      <th>Email</th>
      <th>Password</th>
      <th>Isadmin</th>
      <th>Gender</th>
      <th>Age</th>
      <th>Country</th>
      <th>City</th>
      <th>Show</th>
      <th>Edit</th>
      <th>Destroy</th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.username %></td>
        <td><%= user.email %></td>
        <td><%= user.password %></td>
        <td><%= user.isadmin %></td>
        <td><%= user.gender %></td>
        <td><%= user.age %></td>
        <td><%= user.country %></td>
        <td><%= user.city %></td>
        <td><%= link_to 'Show', user %></td>
        <td><%= link_to 'Edit', edit_user_path(user) %></td>
        <td><%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

        <br>
      </div>
    </div>
  </div>
</div>